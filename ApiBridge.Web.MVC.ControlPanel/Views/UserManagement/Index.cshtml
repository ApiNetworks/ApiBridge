@using ApiBridge.Contracts

@model IEnumerable<ApiBridge.Contracts.User>

@{
    ViewBag.Title = "Index";
}

<h1 class="page-title">
    <i class="icon-home"></i>YAHOO.COM - User Dashboard
</h1>

<div class="widget widget-table">
        <div class="widget-header">
            <i class="icon-th-list"></i>
            <h3>
                Active Registered Users</h3>
        </div>
        <!-- /widget-header -->
        <div class="widget-content">


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                User Name
            </th>
            <th>
                Full Name
            </th>
            <th>
                Email
            </th>
            <th>
                Date Created
            </th>
            <th>
                Last Login
            </th>
        </tr>
    </thead>
<tbody>
@foreach (User user in Model)
{
    <tr>
        <td>@user.Username</td>
        <td>@user.FullName</td>
        <td>@user.Email</td>
        <td>@user.DateCreated</td>
        <td>@user.DateLastLogin</td>
    </tr>
}
</tbody>
</table>

        </div>
</div>
